{
  "name": "th0masso/ssacc-bundle",
  "description": "Access Control Checker Easy Neat Thorough",
  "type": "symfony-bundle",
  "keywords": [
    "security",
    "api-platform",
    "symfony",
    "acl",
    "checks"
  ],
  "require": {
    "php": "^8.0",
    "symfony/config": "^6.0|^7.0",
    "symfony/console": "^6.0|^7.0",
    "symfony/http-kernel": "^6.0|^7.0",
    "symfony/routing": "^6.0|^7.0",
    "symfony/yaml": "^6.0|^7.0",
    "symfony/dependency-injection": "^6.0|^7.0",
    "symfony/security-bundle": "^6.0|^7.0"
  },
  "autoload": {
    "psr-4": {
      "Th0masso\\SsaccBundle\\": "src/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas Hercule",
      "email": "thomash@theodo.fr"
    }
  ],
  "require-dev": {
    "phpstan/phpstan": "^1.8",
    "friendsofphp/php-cs-fixer": "^3.9"
  },
  "config": {
    "allow-plugins": {
      "symfony/runtime": true
    }
  }
}
